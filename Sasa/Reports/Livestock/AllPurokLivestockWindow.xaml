<Window x:Class="Sasa.Reports.Livestock.AllPurokLivestockWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sasa.Reports.Livestock"
        mc:Ignorable="d"
        WindowState="Maximized"
        Title="All Purok Livestock Report" Height="800" Width="550">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>

  
        <ContentControl x:Name="ReportContainer" Grid.Column="1"></ContentControl>
    </Grid>
</Window>
